{
    "manifest_version": 2,
    "name": "BigMacs",
    "version": "1.0",
    "description": "Based on the Big Mac Index.",
    "icons": {
        "16": "images/BigMac-16.png",
        "48": "images/BigMac-48.png",
        "128": "images/BigMac-128.png"
    },
    "content_scripts": [{
        "matches": ["*://*/*"],
        "js": ["script.js"],
        "run_at": "document_end"
    }]
}